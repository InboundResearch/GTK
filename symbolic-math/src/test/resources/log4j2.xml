<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level %-40logger{-4} %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="Console"/>
    </Root>
    <Logger name="us.irdev.gtk.symbolic_math" level="debug" additivity="false" >
      <AppenderRef ref="Console"/>
    </Logger>
  </Loggers>
</Configuration>
